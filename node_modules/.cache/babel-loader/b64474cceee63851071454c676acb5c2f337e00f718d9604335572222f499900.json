{"ast":null,"code":"import Rating from '../StarRating';\nimport Button from '../shared/Button';\nimport { bookApartment } from '../../services/order.service';\nexport default {\n  name: 'ApartmentsMainInfo',\n  components: {\n    Rating,\n    Button\n  },\n  props: {\n    apartment: {\n      type: Object,\n      required: true\n    }\n  },\n  data() {\n    return {\n      isLoading: false\n    };\n  },\n  methods: {\n    async handleApartmentsBooking() {\n      const body = {\n        apartmentId: this.$route.params.id,\n        date: Date.now()\n      };\n      try {\n        this.isLoading = true;\n        await bookApartment(body);\n        this.$notify({\n          type: 'success',\n          title: 'Заказ добавлен'\n        });\n      } catch (error) {\n        this.$notify({\n          type: 'error',\n          title: 'Произошла ошибка',\n          text: error.message\n        });\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["Rating","Button","bookApartment","name","components","props","apartment","type","Object","required","data","isLoading","methods","handleApartmentsBooking","body","apartmentId","$route","params","id","date","Date","now","$notify","title","error","text","message"],"sources":["/Users/andrew/Documents/DOC/IT/Репозиторії/my-Vue-app/src/components/apartment/ApartmentsMainInfo.vue"],"sourcesContent":["<template>\n  <article class=\"apartment-main-info\">\n    <div class=\"apartment-main-info__heading\">\n      <h1 class=\"apartment-main-info__title\">{{ apartment.title }}</h1>\n      <Rating :rating=\"apartment.rating\" />\n    </div>\n    <img :src=\"apartment.imgUrl\" alt=\"\" class=\"apartment-main-info__photo\" />\n    <p class=\"apartment-main-info__description\">{{ apartment.descr }}</p>\n    <div class=\"apartment-main-info__btn\">\n      <Button @click=\"handleApartmentsBooking\" :loading=\"isLoading\">\n        Забронировать\n      </Button>\n    </div>\n  </article>\n</template>\n\n<script>\nimport Rating from '../StarRating';\nimport Button from '../shared/Button';\nimport { bookApartment } from '../../services/order.service';\n\nexport default {\n  name: 'ApartmentsMainInfo',\n  components: {\n    Rating,\n    Button,\n  },\n  props: {\n    apartment: {\n      type: Object,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      isLoading: false,\n    };\n  },\n  methods: {\n    async handleApartmentsBooking() {\n      const body = {\n        apartmentId: this.$route.params.id,\n        date: Date.now(),\n      };\n\n      try {\n        this.isLoading = true;\n        await bookApartment(body);\n\n        this.$notify({\n          type: 'success',\n          title: 'Заказ добавлен',\n        });\n      } catch (error) {\n        this.$notify({\n          type: 'error',\n          title: 'Произошла ошибка',\n          text: error.message,\n        });\n      } finally {\n        this.isLoading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.apartment-main-info {\n  &__heading {\n    display: flex;\n  }\n\n  &__title {\n    font-size: 20px;\n    font-weight: 700;\n    text-transform: capitalize;\n    margin-bottom: 20px;\n    margin-right: 20px;\n  }\n\n  &__photo {\n    max-width: 100%;\n  }\n\n  &__description {\n    line-height: 1.3;\n    margin-top: 30px;\n  }\n\n  &__btn {\n    margin-top: 20px;\n    text-align: center;\n  }\n}\n</style>\n"],"mappings":"AAiBA,OAAOA,MAAK,MAAO,eAAe;AAClC,OAAOC,MAAK,MAAO,kBAAkB;AACrC,SAASC,aAAY,QAAS,8BAA8B;AAE5D,eAAe;EACbC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACVJ,MAAM;IACNC;EACF,CAAC;EACDI,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,uBAAuBA,CAAA,EAAG;MAC9B,MAAMC,IAAG,GAAI;QACXC,WAAW,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;QAClCC,IAAI,EAAEC,IAAI,CAACC,GAAG,CAAC;MACjB,CAAC;MAED,IAAI;QACF,IAAI,CAACV,SAAQ,GAAI,IAAI;QACrB,MAAMT,